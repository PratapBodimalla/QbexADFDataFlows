{
	"name": "dataflow1",
	"properties": {
		"folder": {
			"name": "Unpivot"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_student_marks",
						"type": "DatasetReference"
					},
					"name": "studentMarks"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_adls_folder",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "unpivot1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          school as string,",
				"          class as string,",
				"          {student id} as string,",
				"          student_name as string,",
				"          {subject-1-marks} as string,",
				"          {subject-2-marks} as string,",
				"          {subject-3-marks} as string,",
				"          {subject-4-marks} as string,",
				"          {subject-5-marks} as string,",
				"          {total-marks} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> studentMarks",
				"studentMarks unpivot(output(",
				"          subject as string,",
				"          marks as string",
				"     ),",
				"     ungroupBy(school,",
				"          class,",
				"          {student id},",
				"          student_name),",
				"     lateral: true,",
				"     ignoreNullPivots: false) ~> unpivot1",
				"unpivot1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          COUNTRY as string,",
				"          {COUNTRY CODE} as string,",
				"          {ISO CODES} as string,",
				"          POPULATION as string,",
				"          {AREA KM2} as string,",
				"          GDP_USD as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}